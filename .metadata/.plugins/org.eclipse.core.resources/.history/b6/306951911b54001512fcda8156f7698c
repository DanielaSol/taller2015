#ifndef GAMEOBJECT_H_
#define GAMEOBJECT_H_

#include <iostream>

class GameObject {

public:
	GameObject();
	virtual ~GameObject();
	void draw() { std::cout << "draw game object"; }
	void update() { std::cout << "update game object"; }
	void clean() { std::cout << "clean game object"; }

protected:
	int m_x;
	int m_y;

};

#endif /* GAMEOBJECT_H_ */
